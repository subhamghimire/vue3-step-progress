{
  "name": "vue3-step-progress",
  "description": "Vue 3 step progress bar with compostion api",
  "version": "0.2.2",
  "author": {
    "name": "Subham Ghimire",
    "email": "subham.ghimire26@gmail.com"
  },
  "repository": "https://github.com/subhamghimire/vue3-step-progress.git",
  "bugs": "https://github.com/subhamghimire/vue3-step-progress.git/issues",
  "keywords": [
    "vue3-step-progress",
    "vue3-step",
    "vue-step-composition-api",
    "vue3-step-composition-api",
    "vue-step",
    "vue-step-progress",
    "vue3-step-progress",
    "vue3-step-progressbar",
    "vue3-progress-bar"
  ],
  "license": "MIT",
  "main": "dist/vue3-step-progress.ssr.js",
  "browser": "dist/vue3-step-progress.esm.js",
  "module": "dist/vue3-step-progress.esm.js",
  "unpkg": "dist/vue3-step-progress.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^5.0.1",
    "@vue/cli-service": "^5.0.1",
    "@vue/compiler-sfc": "^3.0.11",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "postcss": "^8.2.10",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^3.0.5"
  },
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "engines": {
    "node": ">=12"
  }
}
